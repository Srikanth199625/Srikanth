package fi;

public class Stringpalindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s= "Madam";
		String f="";
		
		for(int i=s.length()-1; i>=0; i--)
		{
			f=f+ s.charAt(i);
			System.out.println(f);
			//System.out.println(s.charAt(i));
		}

	}

}
